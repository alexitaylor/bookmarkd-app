{
  "name": "server",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "bun run --hot src/index.ts",
    "start": "bun run dist/index.js"
  },
  "dependencies": {
    "elysia": "catalog:",
    "@elysiajs/cors": "^1.4.0",
    "@orpc/server": "catalog:",
    "@orpc/openapi": "catalog:",
    "@orpc/zod": "catalog:",
    "better-auth": "catalog:",
    "ai": "catalog:",
    "@ai-sdk/google": "catalog:",
    "dotenv": "catalog:",
    "zod": "^4.1.13",
    "@bookmarkd/api": "workspace:*",
    "@bookmarkd/auth": "workspace:*",
    "@bookmarkd/db": "workspace:*"
  },
  "trustedDependencies": [
    "supabase"
  ],
  "devDependencies": {
    "typescript": "^5.8.2",
    "@types/bun": "^1.2.6",
    "tsdown": "^0.16.5",
    "@bookmarkd/config": "workspace:*"
  }
}
